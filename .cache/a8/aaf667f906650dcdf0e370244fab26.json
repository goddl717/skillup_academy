plate>\n<div class = \"sheet\">\n   \n    <div class = \"top\">\n        <div class = \"top-title\">\"MOM\"BOX</div>\n        <div class = \"top-main\">\n            <div class = \"top-menu\">메뉴</div>  \n            <div class = \"top-info\">구독</div>\n            <div class = \"top-infoco\" >회사</div>\n        </div>\n    </div>\n\n    <div class = \"middle\" >\n            <div class = \"right-window\">\n                <div>리스트</div>\n            \n            <div class = \"list-view\" v-for = \"(item,index) in items\" :key = \"index\" @click = \"addMoney(index)\">\n                <div>{{item[index,0]}}</div>\n                <button>{{item[index,0]}}</button>\n            </div>\n\n            </div>\n\n            \n            <div class = \"left-window\">\n               <div class = \"platform\">로그인\n                    <div class = \"id-box\">\n                        <div class = \"id-box1\">id</div> \n                        <input  class = \"id-box2 \" type=\"text\"/>\n                        \n                    </div>\n\n                     <div class = \"password-box\">\n                        <div>password</div>\n                        <input class = \"login-password\" type=\"text\"/>\n                    </div>\n\n                    <div class = \"check-box\">\n                        <button class = \"login\">login</button>\n                        <button class = \"sign-up\">sign-up</button>  \n                    </div>\n                    </div>\n                    \n                <div class = \"begged-item\">담은 메뉴 {{fooditem}}</div>\n                <div class = \"view-total\">금액 : {{don}}</div>\n\n            </div>\n    </div>\n</div>\n      \n</template>\n\n<!-- JS  vue 인스턴스. -->\n<!-- 변수를 선언 함수를 선언.-->\n<script>\n    //변수 선언 방법//특이한 방법인거같다.\n    export default{\n        //변수 선언 방법\n        data(){\n            return {\n                addInput :'',\n                items : [[\"불고기\",2000],[\"제육볶음\",200],[\"밥\",500],[\"새우튀김\",1000],[\"김치\",300],[\"단무지\",100],[\"된장국\",500]],\n                don : 0,\n                fooditem : [],\n                temp : \"\",\n            }\n        },\n        //함수 선언 방법 \n        // this.isMenuShow = !this.isMenuShow\n        //로 사용가능 \n        methods :{\n            addTodo(){\n                this.items.push(this.addInput)\n                this.addInput=\"\"\n            },\n            deleteTodo(index){\n              this.items.splice(index,1)\n            },\n            addMoney(index){\n                {\n                    console.log(this.items[index][1])\n                    this.don += this.items[index][1]\n                    this.temp = this.items[index][0]\n                    console.log(this.temp)\n                    this.fooditem.push(this.temp)\n                    \n                    console.log(this.fooditem)\n                }\n\n            },\n            \n        },\n        \n    }\n   \n     \n</script>\n\n<!-- CSS -->\n<style lang = \"scss\">\n.sheet{\n    background-color: rgb(255,247,225);\n.top{\n    border :1.5px solid rgb(190,192,194);\n   \n\n    .top-title{\n        text-align: center ;\n        font-size: 50px;\n    }\n\n    .top-main{\n        border : 1px solid rgb(190,192,194);\n        display: flex;\n        text-align: left;\n        .top-menu{  \n            flex :1;\n        }\n        .top-info{\n         \n            flex :1;\n        }\n        .top-info{\n         \n            flex :1;\n        }\n    }\n\n\n}\n\n.middle{\n    border : 1px solid rgb(190,192,194);\n    display: flex;\n    \n    .list-view{\n    display: inline-block;\n      border : 1px solid rgb(190,192,194);\n\n    }\n    \n    .right-window{\n        border :1px solid rgb(190,192,194);\n        text-align : center;\n        flex : 3;    \n      \n        \n    }\n  \n   .left-window{\n       flex :1;\n         border : 1px solid rgb(190,192,194);\n\n        .platform{\n           .id-box{\n               display:flex;\n           }\n            .password-box{\n                display:flex;\n            }\n        }\n   }\n\n  \n   }\n}\n</style>\n"},"lineCount":null},"css":".sheet {\n  background-color: #fff7e1;\n}\n.sheet .top {\n  border: 1.5px solid #bec0c2;\n}\n.sheet .top .top-title {\n  text-align: center;\n  font-size: 50px;\n}\n.sheet .top .top-main {\n  border: 1px solid #bec0c2;\n  display: flex;\n  text-align: left;\n}\n.sheet .top .top-main .top-menu {\n  flex: 1;\n}\n.sheet .top .top-main .top-info {\n  flex: 1;\n}\n.sheet .top .top-main .top-info {\n  flex: 1;\n}\n.sheet .middle {\n  border: 1px solid #bec0c2;\n  display: flex;\n}\n.sheet .middle .list-view {\n  display: inline-block;\n  border: 1px solid #bec0c2;\n}\n.sheet .middle .right-window {\n  border: 1px solid #bec0c2;\n  text-align: center;\n  flex: 3;\n}\n.sheet .middle .left-window {\n  flex: 1;\n  border: 1px solid #bec0c2;\n}\n.sheet .middle .left-window .platform .id-box {\n  display: flex;\n}\n.sheet .middle .left-window .platform .password-box {\n  display: flex;\n}"},"hash":"96ecd279d0b8d8e9e24bd1d8c12ee6ff","cacheData":{"env":{}}}